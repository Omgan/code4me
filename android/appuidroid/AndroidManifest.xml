<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appuidroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="android.permission.DEVICE_POWER" />
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".AppUiGuideActivity"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity
            android:name=".QQLoginActivity"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="com.example.appuidroid.QQLOGIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
		<activity
            android:name=".QQContactDialActivity"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="com.example.appuidroid.QQCONTACTDIAL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>   
        <activity
            android:name=".SensorMonitorActivity"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="com.example.appuidroid.SENSORDEMO" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>		
        <activity
            android:name=".ViewPagerActivity"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="com.example.appuidroid.VIEWPAGER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PathMainActivity"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="com.example.appuidroid.PATHMAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>    
    </application>
</manifest>